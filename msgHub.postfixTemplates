  java.util.Collection [com.jkopay.msghub.exception.Assertions] -> Assertions.isNotEmpty($expr$).orThrow($end$);
  java.util.Map [com.jkopay.msghub.exception.Assertions] -> Assertions.isNotEmpty($expr$).orThrow($end$);
